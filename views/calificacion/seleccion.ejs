<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<title>Seleccion Mantenimiento</title>
    <link rel="stylesheet" href="https://bootswatch.com/4/minty/bootstrap.min.css">
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/css/select2.min.css" rel="stylesheet" />
</head>
<body class="container" style="margin-top: 50px;">

	<% if (messages.error) { %>
	<div class="alert alert-danger" role="alert"><%- messages.error %></div>
    <% } %>
    <% if (messages.success) { %>
        <div class="alert alert-success" role="alert"><%- messages.success %></div>
    <% } %>
    <div class="card"> 
        <div class="card-header">
            <ul class="nav nav-pills w-100">
                <li class="nav-pill active">
                    <a class="nav-link">Seleccionar Mantenimiento</a>
                </li>
                <li class="nav-pill ml-auto">
                    <a class="nav-link active" href="/user/doctor">Regresar</a>
                </li>
            </ul>
        </div>
    <div class="card-body">
    <div class="card border-secondary"> 
        <div class="card-body">
            <% if(dataAs.length) { %>
                <table class=" table">
                    <thead class = "table-secondary">
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Mantenimiento</th>
                            <th width="200px">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i< dataAs.length; i++) { %>
                        <tr>
                            <th scope="row"><%= (i+1) %></th>
                            <td><%= dataAs[i].nombreman%></td>
                            <td>
                                <a class="btn btn-success edit" href="../calificacion/calificacionesTecnicos/<%=dataAs[i].idMantenimientoAsignacion%>">Calificar Tecnicos</a>               
                            </td>
                        </tr>
                    <% } %>
                    </tbody>
                </table>
            <% } %>
        </div>
    </div>
</body>
</html>